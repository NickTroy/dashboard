<div class="row" style="padding: 8% 17% 37% 17%;">
  <div class="col-lg-12">
    <div class="panel panel-default plain toggle panelMove panelClose panelRefresh">
      <div class="panel-heading">
        <h4 class="panel-title">Variants</h4>
        <%= link_to "+ New variant", new_admin_variant_path,remote: true, style:"float:right;" %>
      </div>
      <div class="panel-body">
        <table id="basic-datatables" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead>
            <tr>
              <th>ID</th>
              <th>SKU</th>
              <th>Barcode</th>
              <th>Weight</th>
              <th>Weight Unit</th>
              <th>Price</th>
              <th>Compare Price</th>
              <th>Master</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody class="variant">
          	<%= render @variants %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>