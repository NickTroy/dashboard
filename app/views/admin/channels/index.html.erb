<div class="row" style="padding: 8% 17% 37% 17%;">
  <div class="col-lg-12">
    <div class="panel panel-default plain toggle panelMove panelClose panelRefresh">
      <div class="panel-heading">
          <h4 class="panel-title">Channels</h4>
          <%= link_to "+ New channel", new_admin_channel_path, remote: true, style:"float:right;" %>
      </div>
      <div class="panel-body">
          <table id="basic-datatables" class="table table-striped table-bordered" cellspacing="0" width="100%">
              <thead>
                  <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th>Created_at</th>
                    <th>Action</th>
                  </tr>
              </thead>
              <tbody class="channel">
                <%= render @channels %>
              </tbody>
          </table>
      </div>
    </div>
  </div>
</div>